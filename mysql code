TABLE 1: Employee
  Column 1: employeeId
  Column 2: employeeName
  
Table 2: Cab
  Column 1: cabId
  Column 2: driverName
  Column 3: availability
  
Table 3: Booking 
  Column 1: bookingId
  Column 2: employeeId
  Column 3: cabId
  Column 4: distance
  Column 5: chargingAmount
  
Table 4: BookingHelper
  Column 1: id
  Column 2: tempBookingId
